{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "üõçÔ∏è–¢–æ–≤–∞—Ä—ã"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "sku": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 1
    },
    "unit": {
      "type": "string",
      "default": "—à—Ç",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "wholesale": {
      "type": "boolean",
      "default": false
    },
    "wholesale_price": {
      "type": "decimal",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "wholesale"
            },
            true
          ]
        }
      }
    },
    "wholesale_min_qty": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "wholesale"
            },
            true
          ]
        }
      }
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "description": {
      "type": "blocks"
    },
    "featured_product": {
      "type": "boolean"
    },
    "linked_products": {
      "type": "component",
      "component": "shared.sku",
      "repeatable": true
    }
  }
}
